[
    {
        "\u4f1a\u8a71": "A\uff1a\ud64d\uc11d\ucc9c\uc774 GD\ud55c\ud14c \ub7ec\ube0c\ucf5c \ubcf4\ub0c8\ub300\nB\uff1a\uc9c4\uc9dc? \ubcc0\uc6b0\uc11d \ucd94\uc601\uc6b0\ub3c4 \ub9cc\ub0ac\uc796\uc544\nA\uff1a\ub9de\uc544 \ub77c\uc774\uc988 \ub354\ubcf4\uc774\uc988\ub3c4 \ubd24\ub300\nB\uff1a\ub300\ubc15 \uc2a4\ud2b8\ub808\uc774\ud0a4\uc988 \ud544\ub9ad\uc2a4\ub3c4 \ubd24\ub2e4\ub2c8\nA\uff1a\ud64d\uc11d\ucc9c \uc778\ub9e5 \uc9c4\uc9dc \ub113\ub2e4 [\uad81\uae08\ud558\ub2e4]\nB\uff1aGD\ub791 \ud64d\uc11d\ucc9c \uc870\ud569 [\uae30\ub300\ub41c\ub2e4]",
        "\u97d3\u56fd\u8a9e": "[1] \uad81\uae08\ud558\ub2e4\uff1a\u6c17\u306b\u306a\u308b\u3001\u77e5\u308a\u305f\u3044\n[2] \uae30\ub300\ub41c\ub2e4\uff1a\u697d\u3057\u307f\u3060\u3001\u671f\u5f85\u3067\u304d\u308b\n[3] \ub300\ubc15\uff1a\u30de\u30b8\u3067\u3001\u3059\u3054\u3044\u3001\u5927\u5f53\u305f\u308a\n[4] \ub7ec\ube0c\ucf5c\uff1a\u30e9\u30d6\u30b3\u30fc\u30eb\n[5] \uc778\ub9e5\uff1a\u4eba\u8108\n"
    },
    {
        "\u4f1a\u8a71": "A\uff1a\ubc29\ucc2c\uc774 \ud328\uc158\uc704\ud06c \ub05d\ub0b4\uace0 \uc785\uad6d\ud588\ub300\nB\uff1a\uc9c4\uc9dc? \uc2a4\ud2b8\ub808\uc774 \ud0a4\uc988 \uc2a4\ucf00\uc904 \ube61\uc138\ub124\nA\uff1a\uc751 \ud53c\uace4\ud558\uaca0\ub2e4",
        "\u97d3\u56fd\u8a9e": "[1] \ube61\uc138\ub2e4\uff1a(\u6642\u9593\u304c)\u304d\u3064\u3044\u3001\u53b3\u3057\u3044\n[2] \uc785\uad6d\uff1a\u5165\u56fd\n[3] \ud328\uc158\uc704\ud06c\uff1a\u30d5\u30a1\u30c3\u30b7\u30e7\u30f3\u30a6\u30a3\u30fc\u30af\n[4] \uc2a4\ucf00\uc904\uff1a\u30b9\u30b1\u30b8\u30e5\u30fc\u30eb\n[5] \ud53c\uace4\ud558\ub2e4\uff1a\u75b2\u308c\u308b\n"
    },
    {
        "\u4f1a\u8a71": "A\uff1a\uc2a4\ud2b8\ub808\uc774\ud0a4\uc988 \uc774\ubc88 \ub2ec \ube0c\ub79c\ub4dc \ud3c9\ud310 5\uc704\ub798\nB\uff1a\uc624 \uc9c4\uc9dc? \uc9c0\ub09c\ub2ec\ubcf4\ub2e4 \uc62c\ub790\ub2e4\ub358\ub370\nA\uff1a\uc751 \uc9c0\uc218\uac00 9% \ub118\uac8c \uc0c1\uc2b9\ud588\ub300 \ub300\ub2e8\ud558\ub2e4\nB\uff1a\uc5ed\uc2dc \uc2a4\ud0a4\uc988! \ucc3d\ube48 \ub7a9 \uc9c4\uc9dc \uba4b\uc788\uc5b4\nA\uff1a\ub9de\uc544 \ubb34\ub300 \uc7a5\uc545\ub825 \ucd5c\uace0\uc57c \uc644\uc804 \uc778\uc815\nB\uff1a\uc55e\uc73c\ub85c\ub3c4 \uacc4\uc18d \uc798 \ub410\uc73c\uba74 \uc88b\uaca0\ub2e4",
        "\u97d3\u56fd\u8a9e": "[1] \ub300\ubc15\uff1a\u3059\u3054\u3044\u3001\u3084\u3070\u3044 (\u611f\u5606\u8a5e)\n[2] \uc778\uc815\uff1a\u8a8d\u3081\u308b\u3001\u540c\u610f\u3059\u308b (\u76f8\u624b\u306e\u610f\u898b\u306b\u8cdb\u6210\u3059\u308b\u3068\u304d)\n[3] \ucd5c\uace0\uff1a\u6700\u9ad8 (\u4e00\u756a\u826f\u3044\u72b6\u614b)\n[4] \uc7a5\uc545\ub825\uff1a\u638c\u63e1\u529b (\u5f71\u97ff\u529b\u3084\u652f\u914d\u529b)\n[5] \uba4b\uc788\ub2e4\uff1a\u304b\u3063\u3053\u3044\u3044 (\u5bb9\u59ff\u3084\u884c\u52d5\u304c\u7d20\u6575)\n"
    }
]